{
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/lp-north-way/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.py"
        }
    ]
}