{
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python",
            "config": { "runtime": "python3.9", "memory": 1024, "maxDuration": 10 }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ]
}
